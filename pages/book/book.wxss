.container{
    display: flex;
    width: 100%;
    flex-direction: column;
    align-items: center;
}
.header{
    position: fixed;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 100rpx;
    box-shadow: 0 0 3px 0 #e3e3e3;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
    background-color: #fff;
    z-index: 99;
}
.box{
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    width: 700rpx;
    height: 68rpx;
    border-radius: 50px;
    background-color: #f5f5f5;
    color: #999999;
}
.box image{
    width: 14px;
    height: 14px;
    margin-right: 10rpx;
    margin-bottom: -2px;
}
.wrapper{
    display: flex;
    margin-top: 100rpx;
    width: 100%;
    flex-direction: column;
    align-items: center;
    background-color:#f5f5f5;
}
.wrapper .wrapper-img{
    width: 106rpx;
    height: 34rpx;
    margin-top: 40rpx;
}
.wrapper-item{
    display: flex;
    margin-top: 10rpx;
    width: 100%;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 0 90rpx;
    box-sizing: border-box;
}
.wrapper-item book{
    margin-bottom: 10rpx;
}